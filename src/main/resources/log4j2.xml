<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" name="DingNet" packages="">
    <Appenders>
        <File name="SignalMoveFile" fileName="logs/moveTo.log" append="false" maxFileSize="100KB">
            <PatternLayout>
                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="SignalMoveFile"/>
        </Root>
    </Loggers>
</Configuration>

